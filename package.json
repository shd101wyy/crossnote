{
  "name": "crossnote",
  "version": "0.8.4",
  "description": "A powerful markdown notebook tool",
  "keywords": [
    "markdown"
  ],
  "repository": "https://github.com/shd101wyy/crossnote",
  "license": "NCSA",
  "contributors": [
    "shd101wyy",
    "kachkaev",
    "gabyx"
  ],
  "exports": {
    ".": {
      "import": "./out/esm/index.mjs",
      "require": "./out/cjs/index.cjs",
      "types": "./out/types/src/index.d.ts"
    }
  },
  "main": "out/cjs/index.cjs",
  "module": "out/esm/index.mjs",
  "types": "out/types/src/index.d.ts",
  "files": [
    "docs",
    "out"
  ],
  "scripts": {
    "build": "rm -rf ./out && yarn copy:files && yarn compile:less && yarn build:typings && yarn build:esbuild",
    "build:esbuild": "node build.js",
    "build:typings": "tsc --project . --declaration",
    "build:watch": "rm -rf ./out && yarn copy:files && yarn compile:less && concurrently \"tsc --project . --watch\" \"node build.js --watch\" \"gulp watch-less\"",
    "check:all": "npm run check:prettier && npm run check:tsc && npm run check:lint",
    "check:lint": "eslint .",
    "check:prettier": "prettier --check \"**/*.*\"",
    "check:tsc": "tsc --project .",
    "compile:less": "gulp compile-less",
    "copy:files": "gulp copy-files",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "prepare": "husky install && npm run build",
    "test": "jest --no-coverage",
    "test:coverage": "jest"
  },
  "lint-staged": {
    "**/*.*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@viz-js/viz": "^3.1.0",
    "async-mutex": "^0.4.0",
    "cheerio": "^1.0.0-rc.12",
    "chrome-paths": "^1.0.1",
    "copy-anything": "^3.0.5",
    "cross-fetch": "^4.0.0",
    "crypto-js": "^4.1.1",
    "html-escaper": "^3.0.3",
    "imagemagick-cli": "^0.5.0",
    "katex": "^0.16.8",
    "less": "^4.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-html5-embed": "^1.0.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "md5": "^2.3.0",
    "mermaid": "^10.4.0",
    "minisearch": "^6.1.0",
    "mkdirp": "^3.0.1",
    "object-hash": "^3.0.0",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "plantuml-encoder": "^1.4.0",
    "puppeteer-core": "^21.1.0",
    "qiniu": "^7.9.0",
    "request": "^2.88.0",
    "slash": "^5.1.0",
    "snake-case": "^3.0.4",
    "temp": "^0.9.0",
    "twemoji": "^13.1.0",
    "type-fest": "^4.3.1",
    "uslug": "^1.0.4",
    "vega": "^5.25.0",
    "vega-lite": "^5.14.1",
    "vega-loader": "^4.5.1",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.11",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.3.29",
    "@types/katex": "^0.16.2",
    "@types/less": "^3.0.4",
    "@types/markdown-it": "^13.0.0",
    "@types/md5": "^2.3.2",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^20.5.7",
    "@types/pako": "^2.0.0",
    "@types/papaparse": "^5.3.8",
    "@types/prismjs": "^1.26.0",
    "@types/temp": "^0.9.1",
    "@types/vfile": "^3.0.2",
    "@types/vscode": "1.80.0",
    "@types/vscode-webview": "^1.57.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^10.3.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "ts-jest": "^27.0.5",
    "typescript": "^5.2.2"
  }
}
