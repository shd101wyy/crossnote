{
  "name": "crossnote",
  "version": "0.9.10",
  "description": "A powerful markdown notebook tool",
  "keywords": [
    "markdown"
  ],
  "repository": "https://github.com/shd101wyy/crossnote",
  "license": "NCSA",
  "contributors": [
    "shd101wyy",
    "kachkaev",
    "gabyx"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": "./out/esm/index.mjs",
      "require": "./out/cjs/index.cjs",
      "types": "./out/types/src/index.d.ts"
    }
  },
  "main": "out/cjs/index.cjs",
  "module": "out/esm/index.mjs",
  "types": "out/types/src/index.d.ts",
  "files": [
    "docs",
    "out"
  ],
  "scripts": {
    "build": "gulp clean-out && yarn copy:files && yarn compile:less && yarn build:typings && yarn build:esbuild",
    "build:esbuild": "node build.js",
    "build:typings": "tsc --project . --declaration",
    "build:watch": "gulp clean-out && yarn copy:files && yarn compile:less && concurrently \"tsc --project . --watch\" \"node build.js --watch\" \"gulp watch-less\"",
    "check:all": "npm run check:prettier && npm run check:tsc && npm run check:lint",
    "check:lint": "eslint .",
    "check:prettier": "prettier --check \"**/*.*\"",
    "check:tsc": "tsc --project .",
    "compile:less": "gulp compile-less",
    "copy:files": "gulp copy-files",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "prepare": "husky install",
    "prepublish": "npm run build",
    "test": "jest --no-coverage",
    "test:coverage": "jest",
    "typedoc": "typedoc src/index.ts"
  },
  "lint-staged": {
    "**/*.*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@monaco-editor/react": "^4.5.2",
    "@sanity/diff-match-patch": "^3.1.1",
    "@ungap/structured-clone": "^1.2.0",
    "@viz-js/viz": "^3.1.0",
    "async-mutex": "^0.4.0",
    "bit-field": "^1.8.0",
    "case-anything": "^2.1.13",
    "cheerio": "^1.0.0-rc.12",
    "chrome-paths": "^1.0.1",
    "classnames": "^2.3.2",
    "copy-anything": "^3.0.5",
    "cross-fetch": "^4.0.0",
    "crypto-js": "^4.1.1",
    "daisyui": "^3.7.3",
    "esbuild-plugin-tailwindcss": "^1.1.1",
    "escape-string-regexp": "^5.0.0",
    "file-saver": "^2.0.5",
    "html-escaper": "^3.0.3",
    "html-react-parser": "^4.2.2",
    "html-to-image": "^1.11.11",
    "imagemagick-cli": "^0.5.0",
    "jquery": "^3.7.1",
    "katex": "^0.16.11",
    "less": "^4.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-html5-embed": "^1.0.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "md5": "^2.3.0",
    "mermaid": "^11.1.1",
    "minisearch": "^6.1.0",
    "mkdirp": "^3.0.1",
    "monaco-editor": "^0.43.0",
    "object-hash": "^3.0.0",
    "onml": "^2.1.0",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "plantuml-encoder": "^1.4.0",
    "puppeteer-core": "^21.1.0",
    "qiniu": "^7.9.0",
    "react": "^18.2.0",
    "react-contexify": "^6.0.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "request": "^2.88.0",
    "simple-icons": "^9.13.0",
    "slash": "^5.1.0",
    "sval": "^0.4.8",
    "temp": "^0.9.0",
    "twemoji": "^13.1.0",
    "type-fest": "^4.3.1",
    "unstated-next": "^1.1.0",
    "uslug": "^1.0.4",
    "vega-loader": "^4.5.1",
    "vscode-uri": "^3.0.7",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/cheerio": "0.22.11",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.6",
    "@types/html-escaper": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/jquery": "^3.3.29",
    "@types/katex": "^0.16.2",
    "@types/less": "^3.0.4",
    "@types/markdown-it": "^13.0.0",
    "@types/md5": "^2.3.2",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^20.5.7",
    "@types/pako": "^2.0.0",
    "@types/papaparse": "^5.3.8",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/temp": "^0.9.1",
    "@types/uslug": "^1.0.2",
    "@types/vfile": "^3.0.2",
    "@types/vscode": "1.80.0",
    "@types/vscode-webview": "^1.57.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^10.3.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  }
}
